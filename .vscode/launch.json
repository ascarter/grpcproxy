    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch proxy",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/proxy",
            "env": {},
            "args": [
                "-cert",
                "${workspaceFolder}/cert.pem",
                "-key",
                "${workspaceFolder}/key.pem",
            ]
        },
        {
            "name": "Launch server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/server",
            "env": {},
            "args": [
                "-cert",
                "${workspaceFolder}/cert.pem",
                "-key",
                "${workspaceFolder}/key.pem",
            ]
        },
        {
            "name": "Launch hello",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/hello",
            "env": {},
            "args": [
                "-address",
                ":50050",
                "-cert",
                "${workspaceFolder}/cert.pem",
                "-key",
                "${workspaceFolder}/key.pem",
            ]
        },
        {
            "name": "Launch echo",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/echo",
            "env": {},
            "args": [
                "-address",
                ":50050",
                "-cert",
                "${workspaceFolder}/cert.pem",
                "-key",
                "${workspaceFolder}/key.pem",
            ]
        }
    ]
}